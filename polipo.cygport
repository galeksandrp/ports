# package name
NAME="privkey"
VERSION=0.1.0
RELEASE=1

# .hint generation
CATEGORY="web"
SUMMARY="T"
DESCRIPTION="P"

# source and patch files
SRC_URI="https://github.com/jech/polipo/archive/polipo-1.1.1.tar.gz"
SRC_DIR="${NAME}-${NAME}-${VERSION}"
DIFF_EXCLUDES="*.html *.info"

src_compile() {
	gcc -o privkey -Iengines/ccgost -Iinclude privkey.c libcrypto.a -pthread -ldl
}

src_install() {
	install privkey /usr/bin
}
